    "yeogurt": "yo yeogurt",
    "module": "yo yeogurt:module",
    "page": "yo yeogurt:page",
    "layout": "yo yeogurt:layout",
    "serve": "concurrently \"gulp serve\" \"npm run api\"",
    "api": "json-server --watch db.json --port 4000"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_images",
      "styles": "_styles",
      "scripts": "_scripts",
      "data": "_data"
    },
    "entries": {
      "js": "main**.js",
      "css": "main**.{sass,scss}"
    }
  },
